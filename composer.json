{
	"name": "silverorange/amqp_pdf_to_text",
	"description": "AMQP worker to convert PDF to plain-text.",
	"type": "library",
	"keywords": [ "PDF", "indexer" ],
	"homepage": "https://github.com/silverorange/amqp-pdf-to-text",
	"license": "MIT",
	"authors": [
		{
			"name": "Michael Gauthier",
			"email": "mike@silverorange.com"
		}
	],
	"bin": [
		"bin/pdf-to-text.php"
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer",
			"options": {
				"ssl": {
					"cafile": "/etc/nginx/tls.crt/composer.crt"
				}
			}
		}
	],
	"require": {
		"php": ">=5.3.0",
		"psr/log": "^1.0.0",
		"silverorange/site": "^4.0.0"
	},
	"require-dev": {
		"silverorange/coding-standard": "^0.1.0"
	},
	"scripts": {
		"post-install-cmd": "./vendor/bin/phpcs --config-set installed_paths vendor/silverorange/coding-standard/src",
		"post-update-cmd": "./vendor/bin/phpcs --config-set installed_paths vendor/silverorange/coding-standard/src"
	},
	"autoload": {
		"psr-0": {
			"AMQP_PDFToText": ""
		}
	},
	"include-path": [
		"./"
	]
}
